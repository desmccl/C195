<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="375.0" prefWidth="625.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.test2.controller.AddCustomer">
   <children>
      <GridPane layoutX="14.0" layoutY="14.0" prefHeight="180.0" prefWidth="284.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="90.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="219.0" minWidth="10.0" prefWidth="194.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Customer ID" />
            <Label text="Customer Name" GridPane.rowIndex="1" />
            <Label text="Address" GridPane.rowIndex="2" />
            <Label text="Postal Code" GridPane.rowIndex="3" />
            <Label text="Phone" GridPane.rowIndex="4" />
            <TextField fx:id="cusid" disable="true" promptText="Auto-Gen Disabled" GridPane.columnIndex="1">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font></TextField>
            <TextField fx:id="cusname" prefHeight="25.0" prefWidth="188.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="cusaddress" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="cuspostcode" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="cusphone" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <ComboBox fx:id="cuscountry" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <ComboBox fx:id="cusdivision" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <Label text="Country" GridPane.rowIndex="5" />
            <Label text="Division" GridPane.rowIndex="6" />
         </children>
      </GridPane>
      <Button fx:id="submit" layoutX="342.0" layoutY="336.0" mnemonicParsing="false" onAction="#onActionSubmit" text="Submit" />
      <Button fx:id="back" layoutX="529.0" layoutY="336.0" mnemonicParsing="false" onAction="#onActionBack" text="Back" />
      <Button fx:id="updatecus" layoutX="437.0" layoutY="336.0" mnemonicParsing="false" onAction="#onActionUpdate" text="Update" />
   </children>
</AnchorPane>
